model Module {
  id       String     @id @default(auto()) @map("_id") @db.ObjectId
  key      String     @unique // ä¾‹ï¼štravel_concern
  title    String
  subtitle String?
  type     ModuleType

  concerns   TravelConcern[]   @relation("ModuleConcerns")
